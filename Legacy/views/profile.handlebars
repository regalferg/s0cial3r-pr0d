<body id="profile-body">
    <video autoplay loop muted preload class="fullscreen-bg_video">
        <source src="/assests/video/Circuit_Background.mp4" type="video/mp4">
    </video>
        <div id="profile-img-container">
            <img src="{{profileImage}}" id="profile-img">     
          
            <h5 class="friend-messages" id="friend-error-message">{{messages.error}}</h5>
            <h5 class="friend-messages" id="friend-add-message">{{messages.info}}</h5>
        </div>
        <div class="row" id="profile-page-header">
            <h1 id="profile-header-background-type">s0cial3r</h1>
        </div>


        <nav class="row" id="profile-nav">
            <ul>
            <li> <a href="/dashboard" id="profile-logo">{{id}}</a></li>
           <li><span id="username-display">{{displayName}}</span></li>
           
            </ul>
            <ul id="nav" class="right hide-on-med-and-down chat-nav-links">
                <li><a href="/profile/{{id}}/general" class="{{class1}}" id="main-chat-link">MAIN CHAT</a></li>
                <li><a href="/profile/{{id}}/gaming_chat" class="{{class2}}" id="gaming-chat-link">GAMING CHAT</a></li>
                <li><a href="/profile/{{id}}/tv_chat" class="{{class3}}" id="tv-chat-link">TV CHAT</a></li>
                <li><a href="/profile/{{id}}/movie_chat" class="{{class4}}"  id="movie-chat-link">MOVIE CHAT</a></li>
                <li><a href="/profile/{{id}}/super-bowl-xxx_giggity" class="{{class5}}" id="sports-chat-link">SPORTS CHAT</a></li>
            </ul>
            <ul id="nav-mobile" class="right hide-on-small-only chat-nav-links">
                <li><a href="/profile/{{id}}/general" class="{{class1}}" id="main-chat-link">MAIN</a></li>
                <li><a href="/profile/{{id}}/gaming_chat" class="{{class2}}" id="gaming-chat-link">GAMING</a></li>
                <li><a href="/profile/{{id}}/tv_chat" class="{{class3}}" id="tv-chat-link">TV</a></li>
                <li><a href="/profile/{{id}}/movie_chat" class="{{class4}}"  id="movie-chat-link">MOVIES</a></li>
                <li><a href="/profile/{{id}}/super-bowl-xxx_giggity" class="{{class5}}" id="sports-chat-link">SPORTS</a></li>
            </ul>
           

        </nav>
        <div class="row" id="search-bar-div-profile">
            <div class="input-field col l6 offset-l3 xl6 offset-xl3" id="post-bar-col">

            </div>
            <div class="col m3" id="logout-button-col">
                <form id ="friendhide"method="post" action="/sendfriend/{{id}}"  class="inline">
                    <input type="hidden" name="submit_param" value={{id}}>
                    <button type="submit" name="submit_param" value="submit_value" class="link-button waves-effect waves-light" id="add-friend">+ ADD FRIEND</button>
                </form>
                <form id ="friendhide"method="post" action="/sendfriend/{{id}}"  class="inline">
                    <input type="hidden" name="submit_param" value={{id}}>
                    <button type="submit" name="submit_param" value="submit_value" class="link-button waves-effect waves-light" id="add-friend-mobile"><i class="material-icons">person_add</i></button>
                </form>
                <a href="/logout" class="waves-effect waves-light btn link-button" id="logout-button"><i class="material-icons left">cloud_off</i>Logout</a>
                <a href="/logout" class="waves-effect waves-light btn link-button" id="logout-button-mobile">Logout</a>
            </div>
        </div>
        <div class="row" id="feed-row">;
        <div class="col m6 offset-m3" id="feed">

        {{#each feed}}
         <div class="card-panel" id="post">
                     <div class="card-content">
                        <a href="#" id="postername">{{username}}</a><span id="says"> says:</span>
                        <br>
                        <br>
                        <span id="post-body">{{message}}</span>
                        <a id="hyp0r"></a>
                        <br><br>
                        <a href="#" id="reply">REPLY</a>
                    </div>
                </div>
        {{/each}}

            </div>    
            </div>

            <div id ="linky" style="display:none;">{{chan}}</div>
        <div class="row" id="profile-page-content">
            <div class="col m6 l3" id="discord-app-col">
                <iframe src="https://discordapp.com/widget?id=406842588239495168&theme=dark" width="350" height="500" allowtransparency="true" frameborder="0" id="discord-widget"></iframe>
            </div>
            <div class="col m6 l6" id="profile-feed-col">

            </div>
            <div class="col m6 l3" id="friends-col">
                <div class="card" id="friends-card">
                    <div class="card-content" id="friends-content">
                        <span class="card-title" id="friends-title">{{displayName}} has <a href="#" id="friends-count"></a> friends</span>
                        <div id="frendo">
                  
                    </div>
                </div>
                
            </div>
            
        </div>
    </div>


